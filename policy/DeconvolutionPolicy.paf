#<?cfg paf policy ?>

# Overrides for Deconvolution, in particular the Alard-Lupton basis
# set.  This sets the order; sigmas are dynamically set in the code
# modifyForImagePsfMatch()
alardNGauss: 4
alardDegGauss: 5 5 3 3

# Typically large residuals right in the core
useCoreStats: false

# Use an extra outer Gaussian?
useOuterForDeconv: false

# Record it being used
modifyForDeconvolution: true
